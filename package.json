{
  "name": "rentalx",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn src/shared/infra/http/server.ts",
    "lint": "eslint . --ext .js,.ts ./src/",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check ./src",
    "lint-staged": "pretty-quick --staged && npm run lint --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/*.ts",
    "seed:admin": "ts-node-dev -r tsconfig-paths/register src/shared/infra/typeorm/seed/admin.ts",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/shared/infra/typeorm/index.ts",
    "typeorm:sync": "typeorm schema:sync",
    "typeorm:drop": "typeorm schema:drop",
    "typeorm:reset": "typeorm:drop && typeorm:sync",
    "typeorm:generate": "typeorm migration:generate",
    "typeorm:create": "typeorm migration:create",
    "test": "set NODE_ENV=test && jest --runInBand --detectOpenHandles"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.388.0",
    "@sentry/integrations": "^7.61.1",
    "@sentry/node": "^7.63.0",
    "@sentry/tracing": "^7.62.0",
    "aws-sdk": "^2.1433.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "csv-parse": "^5.4.0",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.1",
    "mime": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "pg": "^8.11.1",
    "pretty-quick": "^3.1.3",
    "rate-limiter-flexible": "^2.4.2",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.3.3",
    "swagger-ui-express": "^5.0.0",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/node": "^7.22.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "@jest/globals": "^29.6.2",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mime": "^3.0.1",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.9",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.2",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "src/*.{ts,tsx}": [
      "npm run lint"
    ]
  }
}
